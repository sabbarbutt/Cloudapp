{
  "name": "Sabbar cloud-player",
  "version": "4.1.1",
  "description": "Music/video player to play music from SoundCloud and YouTube",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "build": "ng build",
    "release": "ng build --prod && npm run sw && cp CNAME ./dist",
    "lint": "ng lint",
    "postinstall": "ng build --aot -prod",
    "sw": "sw-precache --root=dist --config=swconfig.js"
  },
  "keywords": [
    "angular",
    "angular5",
    "songs",
    "soundcloud",
    "youtube",
    "cloudplayer",
    "sabbar",
    "music",
    "audio",
    "video",
    "player"
  ],
  "engines": {
    "node": "6.11.0",
    "npm": "5.6.0"
  },
  "contributors": [
